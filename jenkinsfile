pipeline{
    agent any
    stages{
        stage ('Configure source code'){
            steps{
                git branch: 'main', url: 'https://github.com/PJWesley7/jenkins_python.git'
            }
        }
        stage ('Install dependcies for python project'){
            steps{
                ansiblePlaybook become: true, disableHostKeyChecking: true, installation: 'ansible', inventory: 'inventory', playbook: 'python-playbook.yml'
            }
        }
    }
}